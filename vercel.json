{
    "version": 2,
    "builds": [
      {
        "src": "backend/run.py",
        "use": "@vercel/python"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/run.py"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/public/$1"
      }
    ]
  }